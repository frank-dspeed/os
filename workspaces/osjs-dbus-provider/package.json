{
  "name": "@osjs/dbus-provider",
  "version": "1.0.6",
  "description": "OS.js DBus Provider",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint src/*.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run eslint && rm ./dist/* && NODE_ENV=production npm run build"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andersevenrud/osjs-dbus-provider.git"
  },
  "keywords": [
    "osjs"
  ],
  "author": "Anders Evenrud <andersevenrud@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andersevenrud/osjs-dbus-provider/issues"
  },
  "homepage": "https://github.com/andersevenrud/osjs-dbus-provider#readme",
  "devDependencies": {
    "@osjs/dev-meta": "^1.1.2"
  },
  "dependencies": {
    "@osjs/event-emitter": "^1.0.9",
    "dbus-native": "^0.2.5",
    "uuid": "^3.3.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@osjs/eslint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
